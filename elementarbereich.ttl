@base <https://w3id.org/openeduhub/vocabs/keyword_catalogue/elementarbereich/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<begriffsbildung> a skos:Concept ;
    skos:broader <vorschulerziehung> ;
    skos:inScheme <> ;
    skos:prefLabel "Begriffsbildung"@de .

<bilderbuch> a skos:Concept ;
    skos:inScheme <> ;
    skos:prefLabel "Bilderbuch"@de ;
    skos:topConceptOf <> .

<kommunikationsförderung> a skos:Concept ;
    skos:broader <vorschulerziehung> ;
    skos:inScheme <> ;
    skos:prefLabel "Kommunikationsförderung"@de .

<krankheit_vorschule> a skos:Concept ;
    skos:inScheme <> ;
    skos:prefLabel "Krankheit (Vorschule)"@de ;
    skos:topConceptOf <> .

<sesamstraße> a skos:Concept ;
    skos:inScheme <> ;
    skos:prefLabel "Sesamstraße"@de ;
    skos:topConceptOf <> .

<vorschulerziehung> a skos:Concept ;
    skos:inScheme <> ;
    skos:narrower <begriffsbildung>,
        <kommunikationsförderung> ;
    skos:prefLabel "Vorschulerziehung"@de ;
    skos:topConceptOf <> .

<> a skos:ConceptScheme ;
    dct:creator "<https://creator.com>" ;
    dct:description "elementarbereich"@de ;
    dct:title "elementarbereich"@de ;
    skos:hasTopConcept <bilderbuch>,
        <krankheit_vorschule>,
        <sesamstraße>,
        <vorschulerziehung> .

