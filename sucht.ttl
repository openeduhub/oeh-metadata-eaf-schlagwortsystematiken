    skos:prefLabel "Rauschgift"@de ;
    skos:topConceptOf <> .

<sucht> a skos:Concept ;
    skos:inScheme <> ;
    skos:narrower <alkoholismus>,
        <arzneimittelmissbrauch>,
        <rauchen> ;
    skos:prefLabel "Sucht"@de ;
    skos:topConceptOf <> .

<> a skos:ConceptScheme ;
    dct:creator "<https://creator.com>" ;
    dct:description "sucht_systematik"@de ;
    dct:title "sucht_systematik"@de ;
    skos:hasTopConcept <drogenberatung>,
        <drogenhandel>,
        <rauschgift>,
        <sucht>,
        <suchtgefahr>,
        <suchtprävention> .

