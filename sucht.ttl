    skos:inScheme <> ;
    skos:prefLabel "Alkoholismus"@de .

<arzneimittelmissbrauch> a skos:Concept ;
    skos:broader <sucht> ;
    skos:inScheme <> ;
    skos:prefLabel "Arzneimittelmissbrauch"@de .

<drogenberatung> a skos:Concept ;
    skos:inScheme <> ;
    skos:prefLabel "Drogenberatung"@de ;
    skos:topConceptOf <> .

<drogenhandel> a skos:Concept ;
    skos:inScheme <> ;
    skos:prefLabel "Drogenhandel"@de ;
    skos:topConceptOf <> .

<ecstasy> a skos:Concept ;
    skos:broader <rauschgift> ;
    skos:inScheme <> ;
    skos:prefLabel "Ecstasy"@de .

<heroin> a skos:Concept ;
    skos:broader <rauschgift> ;
    skos:inScheme <> ;
    skos:prefLabel "Heroin"@de .

<rauchen> a skos:Concept ;
    skos:broader <sucht> ;
    skos:inScheme <> ;
    skos:prefLabel "Rauchen"@de .

<suchtgefahr> a skos:Concept ;
    skos:inScheme <> ;
    skos:prefLabel "Suchtgefahr"@de ;
    skos:topConceptOf <> .

<suchtprävention> a skos:Concept ;
    skos:inScheme <> ;
    skos:prefLabel "Suchtprävention"@de ;
    skos:topConceptOf <> .

<rauschgift> a skos:Concept ;
    skos:inScheme <> ;
    skos:narrower <ecstasy>,
